{
  "name": "aurelia-ui-framework",
  "description": "An Aurelia UI/UX framework",
  "version": "5.0.0-beta.2",
  "keywords": [
    "ui-framework",
    "desktop-framework",
    "aurelia-ui-framework"
  ],
  "homepage": "https://aurelia-ui-framework.herokuapp.com",
  "bugs": {
    "url": "https://github.com/adarshpastakia/aurelia-ui-framework/issues"
  },
  "license": "MIT",
  "author": "Adarsh Pastakia <adarshpastakia@outlook.com> (http://adarshpastakia.github.io/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/adarshpastakia/aurelia-ui-framework"
  },
  "dependencies": {
    "aurelia-binding": "^2.3.1",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-framework": "^1.3.1",
    "aurelia-templating": "^1.10.2",
    "aurelia-ui-virtualization": "^1.0.0-beta.5",
    "aurelia-validation": "^1.4.0",
    "date-fns": "^2.0.0-alpha.27",
    "kramed": "^0.5.6",
    "libphonenumber-js": "^1.7.16",
    "numeral": "^2.0.6",
    "resize-observer-polyfill": "^1.5.1"
  },
  "optionalDependencies": {
    "@mdi/font": "^3.4.93",
    "prismjs": "^1.15.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@types/jest": "^23.3.5",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.14.6",
    "@types/webpack": "^4.4.29",
    "acorn": "^6.1.1",
    "acorn-dynamic-import": "^4.0.0",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-cli": "^1.0.0-beta.15",
    "aurelia-loader-nodejs": "^1.0.1",
    "aurelia-pal-nodejs": "^1.1.1",
    "aurelia-polyfills": "^1.3.4",
    "aurelia-testing": "^1.0.0",
    "aurelia-tools": "^2.0.0",
    "aurelia-webpack-plugin": "^3.0.0",
    "babel-jest": "^24.8.0",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "dts-bundle-generator": "^2.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^2.0.0",
    "gulp-rename": "^1.4.0",
    "html-loader": "^0.5.5",
    "html-loader-jest": "^0.2.1",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.3",
    "minimatch": "^3.0.4",
    "node-sass": "^4.12.0",
    "opn": "^5.5.0",
    "plugin-error": "^1.0.1",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-order": "^2.2.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "through2": "^2.0.3",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-webpack-plugin": "^2.0.4",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.1",
    "vinyl-fs": "^3.0.3",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "latest",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "testFailureExitCode": 0,
    "moduleNameMapper": {
      "^aurelia-binding$": "<rootDir>/node_modules/aurelia-binding",
      "^aurelia-ui-framework$": "<rootDir>/src/aurelia-ui-framework.ts",
      "^src/(.*)": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js?$": "babel-jest",
      "^.+\\.html$": "html-loader-jest"
    },
    "testRegex": "\\.spec\\.(ts|js)$",
    "setupFiles": [
      "<rootDir>/test/jest-setup.ts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/jest-pretest.ts",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/libs/*",
      "!**/*.spec.{js,ts}",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "start": "au run --watch",
    "lint": "tslint 'src/**/*.ts' -p ./",
    "test": "jest --ci --runInBand --verbose --detectOpenHandles",
    "test-watch": "jest --watch",
    "build": "au build --env prod",
    "heroku-postbuild": "au build --env dev",
    "bump-version": "npm --no-git-tag-version version",
    "modules": "webpack --display-modules",
    "analyze": "webpack --env.analyze --env.production",
    "predist": "rimraf dist",
    "dist": "rollup -c",
    "postdist": "tsc -p tsconfig.build.json && mkdir ./dist/typings/libs && cp ./src/libs/*.d.ts ./dist/typings/libs/.",
    "release": "npm run dist && npm run bump-version"
  },
  "typings": "dist/typings/aurelia-ui-framework.d.ts",
  "main": "dist/commonjs/aurelia-ui-framework.js",
  "module": "dist/es2015/aurelia-ui-framework.js",
  "browser": "dist/umd/aurelia-ui-framework.js"
}
